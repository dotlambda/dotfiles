set font "ProFont 8"

set default-fg "#dcdccc"
set default-bg "#000000"
set inputbar-fg "#274266"
set inputbar-bg "#dcdccc"
set statusbar-fg "#dcdccc"
set statusbar-bg "#274266"

set guioptions ""   # Don't show statusbar etc

set recolor true    # Invert colors
set recolor-darkcolor "#dcdccc"
set recolor-lightcolor "#101b2a"
